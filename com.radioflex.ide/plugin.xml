<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.radioflex.ide.ui.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="com.radioflex.ide.ui.Perspective"
            id="com.red.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="File">
            <command
                  commandId="org.eclipse.ui.newWizard"
                  label="New"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.edit.text.openLocalFile"
                  label="Open File ..."
                  style="push">
            </command>
            <separator
                  name="com.radioflex.ide.separator1"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.file.close"
                  label="Close"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.closeAll"
                  label="Close All"
                  style="push">
            </command>
            <separator
                  name="com.radioflex.ide.separator2"
                  visible="true">
            </separator>
            <command
                  commandId="edu.suda.ide.command.save"
                  label="Save"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.saveAs"
                  label="Save As ..."
                  style="push">
            </command>
            <separator
                  name="com.radioflex.ide.separator3"
                  visible="true">
            </separator>
            <menu
                  label="Project Setting ...">
               <command
                     commandId="com.radioflex.ide.command.buildsetting"
                     label="Build Setting"
                     style="push">
               </command>
               <command
                     commandId="com.radioflex.ide.command.debugsetting"
                     label="Debug Setting"
                     style="push">
               </command>
            </menu>
            <command
                  commandId="org.eclipse.ui.file.import"
                  label="Import"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.export"
                  label="Export"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.exit"
                  label="Exit ..."
                  style="push">
            </command>
         </menu>
         <menu
               label="Editor">
            <command
                  commandId="com.radioflex.ide.command.redo"
                  label="Redo"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.undo"
                  label="Undo"
                  style="push">
            </command>
            <separator
                  name="com.radioflex.ide.separator4"
                  visible="true">
            </separator>
            <command
                  commandId="com.radioflex.ide.command.copy"
                  label="Copy"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.paste"
                  label="Paste"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.cut"
                  label="Cut"
                  style="push">
            </command>
            <separator
                  name="com.radioflex.ide.separator5"
                  visible="true">
            </separator>
            <command
                  commandId="com.radioflex.ide.command.findandreplace"
                  label="Find and Replace"
                  style="push">
            </command>
         </menu>
         <menu
               label="Debug">
            <command
                  commandId="com.radioflex.ide.command.build"
                  label="Build"
                  style="push">
            </command>
            <separator
                  name="com.radioflex.ide.separator6"
                  visible="true">
            </separator>
            <command
                  commandId="com.radioflex.ide.command.start"
                  label="Start"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.stop"
                  label="Stop"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.continue"
                  label="Continue"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.step"
                  label="Step"
                  style="push">
            </command>
            <separator
                  name="com.radioflex.ide.separator7"
                  visible="true">
            </separator>
            <command
                  commandId="com.radioflex.ide.command.setbreakpoint"
                  label="Set Breakpoint"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.removebreakpoint"
                  label="Remove Breakpoint"
                  style="push">
            </command>
            <separator
                  name="com.radioflex.ide.separator8"
                  visible="true">
            </separator>
            <command
                  commandId="com.radioflex.ide.command.switchmodel"
                  label="Switch Model"
                  style="push">
            </command>
         </menu>
         <menu
               label="Help">
         </menu>
         <menu
               label="Preference">
            <command
                  commandId="org.eclipse.ui.window.preferences"
                  label="Preference"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="com.radioflex.ide.toolbar1">
            <command
                  commandId="com.radioflex.ide.command.newproject"
                  label="1"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.openproject"
                  label="2"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.newfile"
                  label="3"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.openfile"
                  label="4"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.savefile"
                  label="5"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.build"
                  label="6"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command31"
                  label="7"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command32"
                  label="8"
                  style="push">
            </command>
            <command
                  commandId="com.radioflex.ide.command.step"
                  label="9"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.radioflex.ide.ui.handlers.NewFileHandler"
            commandId="com.radioflex.ide.command.newfile">
      </handler>
      <handler
            class="com.radioflex.ide.ui.handlers.NewProjectHandler"
            commandId="com.radioflex.ide.command.newproject">
      </handler>
      <handler
            class="com.radioflex.ide.ui.handlers.OpenFileHandler"
            commandId="com.radioflex.ide.command.openfile">
      </handler>
      <handler
            class="com.radioflex.ide.ui.handlers.SaveAsHandler"
            commandId="com.radioflex.ide.command.saveas">
      </handler>
      <handler
            class="com.radioflex.ide.ui.handlers.OpenProjectHandler"
            commandId="com.radioflex.ide.command.openproject">
      </handler>
      <handler
            class="com.radioflex.ide.ui.handlers.ExitHandler"
            commandId="com.radioflex.ide.command.exit">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.radioflex.ide.ui.editor.RadioflexEditor"
            default="false"
            extensions="asm"
            id="com.radioflex.ide.editor1"
            name="RadioflexEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="Project Explorer"
            class="org.eclipse.ui.navigator.CommonNavigator"
            id="example.view">
      </view>
   </extension>
    <extension
         point="org.eclipse.ui.navigator.viewer">
       <viewerContentBinding 
            viewerId="example.view">
          <includes>
             <contentExtension
                   pattern="com.ide.projectExplorer.navigatorContent">
             </contentExtension>
          </includes>
       </viewerContentBinding>
       <viewerActionBinding
             viewerId="example.view">
          <includes>
             <actionExtension
                   pattern="org.eclipse.ui.navigator.resources.*">
             </actionExtension>
          </includes>
       </viewerActionBinding>
   </extension>
    <extension
          point="org.eclipse.ui.navigator.navigatorContent">
       <navigatorContent
             contentProvider="com.radioflex.ide.ui.navigator.CustomContentProvider"
             id="com.ide.projectExplorer.navigatorContent"
             labelProvider="com.radioflex.ide.ui.navigator.CustomLabelProvider"
             name="IDE Navigator Content">
          <triggerPoints>
             <instanceof
                   value="org.eclipse.core.resources.IWorkspaceRoot">
             </instanceof>
          </triggerPoints>
       </navigatorContent>
    </extension>
    <extension
          point="org.eclipse.ui.preferencePages">
       <page
             class="com.radioflex.ide.ui.preference.PreferencesGeneral"
             id="com.radioflex.ide.page1"
             name="Prefrence">
       </page>
    </extension>
    <extension
          point="org.eclipse.core.runtime.preferences">
       <initializer
             class="com.radioflex.ide.ui.preference.PreferencesInitializer">
       </initializer>
    </extension>

</plugin>
